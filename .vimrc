" esc in insert & visual mode
imap kj <esc>

set backup
set clipboard+=unnamedplus
set undofile
set undodir=~/.vim/undodir
set nu
set relativenumber
set tabstop=4
set softtabstop=4
set shiftwidth=4
set autoindent
set smartindent
set expandtab
set guicursor=
set scrolloff=5
set updatetime=50
syntax enable
colorscheme gruvbox
